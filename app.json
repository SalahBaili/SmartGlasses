{
  "expo": {
    "name": "apppfa",
    "slug": "apppfa",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "plugins": [
    "expo-image-picker"
  ],
    
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "infoPlist": {
      "NSPhotoLibraryUsageDescription": "Cette application a besoin d’accéder à vos photos pour changer l’image de profil."
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.apppfa"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
